{
  "name": "wellnest",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev --turbopack",
    "dev:mobile": "next dev --turbopack --hostname 0.0.0.0",
    "prebuild": "yarn build:screen-time-monitor",
    "build": "NODE_OPTIONS='--max-old-space-size=8192' next build",
    "build:mobile": "NODE_OPTIONS='--max-old-space-size=8192' next build && npx cap sync",
    "build:prod": "NODE_ENV=production NODE_OPTIONS='--max-old-space-size=8192' next build && npx cap sync",
    "postinstall": "yarn build:screen-time-monitor",
    "build:screen-time-monitor": "cd src/shared/lib/screen-time-monitor && yarn install && yarn build",
    "start": "next start",
    "lint": "next lint",
    "format": "prettier -c \"**/*.{css,js,json,jsx,ts,tsx}\"",
    "format:fix": "prettier -c \"**/*.{css,js,json,jsx,ts,tsx}\" --write",
    "format:fix:src": "prettier -c './src/**/*.{ts,tsx,js}' --write",
    "generate-icons": "node scripts/generate-icons.js",
    "pwa-audit": "npx lighthouse http://localhost:3000 --only-categories=pwa --chrome-flags='--headless'",
    "analyze": "cross-env ANALYZE=true next build",
    "mobile:sync": "npx cap sync",
    "mobile:android": "npx cap run android",
    "mobile:android:dev": "npx cap run android --livereload --external",
    "mobile:android:prod": "NODE_ENV=production npx cap run android",
    "mobile:clean": "npx cap clean android",
    "mobile:doctor": "npx cap doctor",
    "mobile:open": "npx cap open android",
    "mobile:setup": "node scripts/mobile-setup.js",
    "db:test": "tsx src/shared/scripts/db-connection.ts",
    "db:generate": "drizzle-kit generate",
    "db:migrate": "drizzle-kit migrate",
    "db:reset": "tsx src/shared/db/reset.ts",
    "db:seed": "tsx src/shared/db/seed.ts",
    "db:setup": "yarn run db:reset && yarn --silent db:generate && yarn --silent db:migrate && yarn --silent db:seed",
    "db:inspect": "drizzle-kit introspect"
  },
  "dependencies": {
    "@capacitor/android": "^7.4.3",
    "@capacitor/cli": "^7.4.3",
    "@capacitor/core": "^7.4.3",
    "screen-time-monitor": "file:./src/shared/lib/screen-time-monitor",
    "@radix-ui/react-avatar": "^1.1.10",
    "@radix-ui/react-checkbox": "^1.3.2",
    "@radix-ui/react-dialog": "^1.1.14",
    "@radix-ui/react-dropdown-menu": "^2.1.15",
    "@radix-ui/react-label": "^2.1.7",
    "@radix-ui/react-progress": "^1.1.7",
    "@radix-ui/react-select": "^2.2.5",
    "@radix-ui/react-separator": "^1.1.7",
    "@radix-ui/react-slot": "^1.2.3",
    "@radix-ui/react-switch": "^1.2.5",
    "@radix-ui/react-toggle-group": "^1.1.10",
    "@radix-ui/react-tooltip": "^1.2.7",
    "@tanstack/react-query": "^5.83.0",
    "bcryptjs": "^3.0.2",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "date-fns": "^4.1.0",
    "dotenv": "^17.2.0",
    "drizzle-orm": "^0.44.3",
    "jspdf": "^3.0.1",
    "jspdf-autotable": "^5.0.2",
    "lucide-react": "^0.532.0",
    "next": "15.3.5",
    "next-auth": "^4.24.11",
    "next-pwa": "^5.6.0",
    "next-themes": "^0.4.6",
    "pg": "^8.16.3",
    "pg-core": "^0.1.15",
    "postgres": "^3.4.7",
    "prettier": "^3.6.2",
    "prettier-plugin-tailwindcss": "^0.6.14",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-icons": "^5.5.0",
    "react-is": "^19.1.0",
    "react-toastify": "^11.0.5",
    "recharts": "^3.1.0",
    "styled-jsx": "^5.1.7",
    "tailwind-merge": "^3.3.1",
    "workbox-background-sync": "^7.3.0",
    "workbox-cacheable-response": "^7.3.0",
    "workbox-expiration": "^7.3.0",
    "workbox-precaching": "^7.3.0",
    "workbox-routing": "^7.3.0",
    "workbox-strategies": "^7.3.0",
    "workbox-window": "^7.3.0",
    "xlsx": "^0.18.5",
    "zod": "^4.0.5",
    "zustand": "^5.0.6"
  },
  "devDependencies": {
    "@tailwindcss/postcss": "^4",
    "@tanstack/react-query-devtools": "^5.83.0",
    "@types/bcryptjs": "^3.0.0",
    "@types/jspdf": "^2.0.0",
    "@types/node": "^24.0.13",
    "@types/pg": "^8.15.4",
    "@types/react": "^19.1.10",
    "@types/react-dom": "^19.1.7",
    "cross-env": "^7.0.3",
    "drizzle-kit": "^0.31.4",
    "eslint": "^9",
    "eslint-config-next": "15.4.6",
    "sharp": "^0.33.5",
    "tailwindcss": "^4",
    "tsx": "^4.20.3",
    "typescript": "^5"
  }
}
